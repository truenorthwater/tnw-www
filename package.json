{
  "name": "true-north-water",
  "description": "True North Water",
  "version": "1.0.0",
  "author": "True North Water",
  "dependencies": {
    "autoprefixer": "^9.3.1",
    "aws-cloudfront-invalidate": "^0.1.3",
    "better-npm-run": "^0.1.1",
    "browserslist": "^4.3.4",
    "gatsby": "^2.0.0",
    "gatsby-plugin-favicon": "^3.1.5",
    "gatsby-plugin-google-analytics": "^2.0.8",
    "gatsby-plugin-manifest": "^2.0.2",
    "gatsby-plugin-offline": "^2.0.5",
    "gatsby-plugin-react-helmet": "^3.0.0",
    "gatsby-plugin-sass": "^2.0.5",
    "node-sass": "^4.10.0",
    "react": "^16.5.1",
    "react-dom": "^16.5.1",
    "react-helmet": "^5.2.0",
    "s3-deploy": "^1.2.0"
  },
  "keywords": [
    "gatsby"
  ],
  "license": "MIT",
  "scripts": {
    "build": "gatsby build",
    "develop": "gatsby develop",
    "format": "prettier --write '**/*.js'",
    "test": "echo \"Error: no test specified\" && exit 1",
    "deploy:dev": "better-npm-run deploy:dev",
    "deploy:prd": "better-npm-run deploy:prd"
  },
  "betterScripts": {
    "deploy:dev": "gatsby build --prefix-paths && s3-deploy './public/**' --cwd './public/' --bucket tnw-www-gatsby-dev --region=us-east-2 --deleteRemoved --gzip && aws-cloudfront-invalidate E1T9RN63T9U8PX",
    "deploy:prd": "gatsby build --prefix-paths && s3-deploy './public/**' --cwd './public/' --bucket tnw-www-gatsby-prd --region=us-east-2 --deleteRemoved --gzip && aws-cloudfront-invalidate E1T9RN63T9U8PX"
  },
  "devDependencies": {
    "prettier": "^1.14.2"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/gatsbyjs/gatsby-starter-default"
  }
